    nginx.ingress.kubernetes.io/upstream-hash-by: "$uid"
    nginx.ingress.kubernetes.io/rewrite-target: /$2
spec:
  rules:
  - host: dev-enterprisek8.skillclash.com
    http:
      paths:
        -
